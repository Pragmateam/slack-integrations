{
  "name": "slack-integrations",
  "version": "1.0.0",
  "description": "Integrations for Slack",
  "keywords": [
    "slack",
    "integrations",
    "serverless",
    "typescript"
  ],
  "author": {
    "name": "Pragmateam Pty Ltd",
    "email": "talent@pragma.team",
    "url": "https://pragma.team"
  },
  "contributors": [
    {
      "name": "Igor J. Santos <igor@pragma.team>",
      "url": "http://github.com/igorjs"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "postinstall": "husky install",
    "clean": "shx rm -rf node_modules .serverless .esbuild .cache",
    "dev": "serverless offline start",
    "start": "concurrently -c \"auto\" \"npm run dev\" \"npm run tunnel\"",
    "tunnel": "lt --port 3000 --subdomain slack-integrations",
    "lint": "eslint src/**",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "codestyle": "prettier --check \"./**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --runInBand --passWithNoTests --coverage --watchAll=false",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@slack/types": "^2.8.0",
    "@slack/web-api": "^6.8.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.54.1",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "localtunnel": "^2.0.2",
    "prettier": "^2.8.4",
    "semantic-release": "^19.0.5",
    "serverless": "^3.28.1",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-offline": "^12.0.4",
    "serverless-webpack": "^5.11.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "webpack": "^5.76.2",
    "webpack-node-externals": "^3.0.0"
  }
}
