{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "trailingComma": "es5",
  "arrowParens": "always",
  "proseWrap": "never",
  "importOrder": [
    "^(angular|react)(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@src/(.*)$",
    "^[../]",
    "^./app",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrderGroupNamespaceSpecifiers": true
}
